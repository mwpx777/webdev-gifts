 else {\n            dispatch({\n                type: ADD_TO_CART,\n\n                product: { ...currentProduct, purchaseQuantity: 1, size: value, }\n            });\n            idbPromise('cart', 'put', { ...currentProduct, purchaseQuantity: 1 });\n        };\n    };\n\n    const removeFromCart = () => {\n        dispatch({\n            type: REMOVE_FROM_CART,\n            _id: currentProduct._id\n        });\n        idbPromise('cart', 'delete', { ...currentProduct });\n    };\n    useEffect(() => {\n        // products already in globalStore\n        if (products.length) {\n            console.log((products.find(product => product._id === id)))\n            setCurrentProduct(products.find(product => product._id === id));\n\n            // retrieved from server\n        } else if (data) {\n\n            dispatch({\n                type: UPDATE_PRODUCTS,\n                products: data.products\n            });\n\n            data.products.forEach(product => {\n                idbPromise('products', 'put', product);\n            });\n\n\n            // get cache from idb\n        } else if (!loading) {\n            idbPromise('products', 'get').then((products) => {\n                dispatch({\n                    type: UPDATE_PRODUCTS,\n                    products: products\n                });\n            });\n        }\n    }, [products, data, loading, dispatch, id]);\n\n\n    const handleSelect = (e) => {\n        console.log(e.target.value);\n        setValue(e.target.value);\n    }\n    \n\n\n    return (\n        <>\n            {currentProduct ? (\n                <div className=\"container-fluid\">\n                    <Link to=\"/\" id=\"backLink\">\n                        <span id=\"back\"> ‚Üê Back to Products</span>\n                    </Link>\n                    <div className=\"detail-row animate__animated animate__lightSpeedInLeft\">\n                        <h1>{currentProduct.name}</h1>\n                    </div>\n                    <div className=\"detail-row\">\n                        <p>{currentProduct.description}</p>\n                    </div>\n                    <div className=\"detail-row\">\n\n                        <img src={`/images/${currentProduct.image}`} alt={currentProduct.name} id=\"detailImage\" className=\"animate__animated animate__backInDown\" />\n                    </div>\n                    <div className= \"col-5\">\n                        <h1 className= \"animate__animated animate__lightSpeedInRight\">{currentProduct.name}</h1>\n                        <h3>Price: ${currentProduct.price}{\" \"}</h3>\n                    <div className=\"row justify-content-start py-4\">\n                        \n                    {\n                        currentProduct._id == \"60a6f97604860b499ce41857\" || currentProduct._id == \"60a6f97604860b499ce41856\" ?\n                            (\n                                <div className=\"detail-row\">\n                                    <h3>Choose a size </h3>\n                                    <select className=\"form-select\" aria-label=\"Default select example\" onChange={handleSelect} id=\"dropdown\">\n                                        <option value=\"empty\">Select a size</option>\n                                        <option value=\"Small\">Small</option>\n                                        <option value=\"Medium\">Medium</option>\n                                        <option value=\"Large\">Large</option>\n                                        <option value=\"XL\">XL</option>\n                                    </select>\n                                </div>\n                            ) : null\n                    }\n                    {\n\n                    }\n\n                    <div className=\"detail-row\"><h2>Price: ${currentProduct.price}{\" \"}</h2>\n                        <button className=\"hvr-pulse-grow\" id=\"addToCartBtn\" onClick={addToCart}>\n                            Add To Cart\n                        </button>\n                        </div>\n                        </div>\n                        \n                    <div>\n                        <h3>Product Details:</h3>\n                    <p>{currentProduct.description}</p>\n                    </div>\n                    </div>\n                    </div>\n               \n\n            ) : null}\n            {\n                loading ? <img src={spinner} alt=\"loading spinner\" /> : null\n            }\n            <Cart />\n        </>\n    );\n};\n\nexport default Detail;","D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\components\\Nav\\index.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\components\\Footer\\index.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\utils\\auth.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\utils\\queries.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\utils\\helpers.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\utils\\reducers.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\utils\\mutations.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\components\\ProductList\\index.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\components\\Jumbotron\\index.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\components\\Cart\\index.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\utils\\actions.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\components\\CategoryMenu\\index.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\components\\CartItem\\index.js",["122","123","124"],"// CART ITEM\n\nimport React from 'react';\nimport { useStoreContext } from '../../utils/GlobalState';\nimport { REMOVE_FROM_CART, UPDATE_CART_QUANTITY } from '../../utils/actions';\nimport { idbPromise } from '../../utils/helpers';\nimport { isTypeExtensionNode } from 'graphql';\n\nconst CartItem = ({ item }) => {\n    const [, dispatch] = useStoreContext();\n\n    const removeFromCart = item => {\n        dispatch({\n            type: REMOVE_FROM_CART,\n            _id: item._id\n        });\n        // remove item from indexedDB\n        idbPromise('cart', 'delete', { ...item });\n    }\n\n    const onChange = (e) => {\n        const value = e.target.value;\n\n        // if number set to zero, remove item from cart\n        if (value === '0') {\n            dispatch({\n                type: REMOVE_FROM_CART,\n                _id: item._id\n            });\n            idbPromise('cart', 'delete', { ...item });\n        } else {\n            dispatch({\n                type: UPDATE_CART_QUANTITY,\n                _id: item._id,\n                purchaseQuantity: parseInt(value)\n            });\n            idbPromise('cart', 'put', { ...item, purchaseQuantity: parseInt(value) });\n        }\n    };\n\n    return (\n        <div className=\"flex-row allCart\" >\n            <div>\n                <img src={`/images/${item.image}`} alt={item.name} id=\"cartImage\" />\n            </div>\n            <div>\n                \n                <div className=\"allCart\">{item.name} </div>\n                <div className=\"allCart\">${item.price}</div>\n                {\n                    item.name == \"WebDev 2020-2021 Tank\" || item.name ==\"WebDev 2020-2021 Tshirt\" ?\n                    (\n                <div className=\"allCart\">Size: {item.size}</div>\n                    ) : null\n                }\n                <div>\n                    <span className=\"allCart\">Quantity: </span>\n                    <input\n                    className=\"allCart\"\n                        type=\"number\"\n                        placeholder=\"1\"\n                        value={item.purchaseQuantity}\n                        onChange={onChange} />\n                    <span\n                        onClick={() => removeFromCart(item)}\n                        role=\"img\"\n                        aria-label=\"trash\"\n                        id=\"trashCan\">\n                        üóëÔ∏è\n                </span>\n                </div>\n            </div>\n        </div>\n    );\n};\n\nexport default CartItem;","D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\components\\ProductItem\\index.js",["125"],"// PRODUCT ITEM\n\nimport React from 'react';\nimport { Link } from 'react-router-dom';\n// import { pluralize } from '../../utils/helpers';\nimport { useStoreContext } from '../../utils/GlobalState';\nimport { ADD_TO_CART, UPDATE_CART_QUANTITY } from '../../utils/actions';\nimport { idbPromise } from '../../utils/helpers';\n\nfunction ProductItem(item) {\n    const {\n        image,\n        name,\n        _id,\n        price,\n    } = item;\n\n    const [state, dispatch] = useStoreContext();\n    const { cart } = state;\n\n    const addToCart = () => {\n        const itemInCart = cart.find((cartItem) => cartItem._id === _id)\n        if (itemInCart) {\n            dispatch({\n                type: UPDATE_CART_QUANTITY,\n                _id: _id,\n                purchaseQuantity: parseInt(itemInCart.purchaseQuantity) + 1\n            });\n            idbPromise('cart', 'put', {\n                ...itemInCart, purchaseQuantity: parseInt(itemInCart.purchaseQuantity) + 1\n            });\n        } else {\n            dispatch({\n                type: ADD_TO_CART,\n                product: { ...item, purchaseQuantity: 1 }\n            });\n            idbPromise('cart', 'put', { ...item, purchaseQuantity: 1 });\n        }\n    }\n\n    return (\n        <div className=\"card px-1 py-1 col-md-3\" id=\"hvr-float-shadow\">\n     