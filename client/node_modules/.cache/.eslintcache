T_QUANTITY,\n                _id: id,\n                purchaseQuantity: parseInt(itemInCart.purchaseQuantity) + 1\n            });\n\n            idbPromise('cart', 'put', {\n                ...itemInCart,\n                purchaseQuantity: parseInt(itemInCart.purchaseQuantity) + 1\n            })\n        } else {\n            dispatch({\n                type: ADD_TO_CART,\n                product: { ...currentProduct, purchaseQuantity: 1 }\n            });\n            idbPromise('cart', 'put', { ...currentProduct, purchaseQuantity: 1 });\n        };\n    };\n\n    const removeFromCart = () => {\n        dispatch({\n            type: REMOVE_FROM_CART,\n            _id: currentProduct._id\n        });\n        idbPromise('cart', 'delete', { ...currentProduct });\n    };\n    useEffect(() => {\n        // products already in globalStore\n        if (products.length) {\n            setCurrentProduct(products.find(product => product._id === id));\n\n            // retrieved from server\n        } else if (data) {\n            dispatch({\n                type: UPDATE_PRODUCTS,\n                products: data.products\n            });\n\n            data.products.forEach(product => {\n                idbPromise('products', 'put', product);\n            });\n\n\n            // get cache from idb\n        } else if (!loading) {\n            idbPromise('products', 'get').then((products) => {\n                dispatch({\n                    type: UPDATE_PRODUCTS,\n                    products: products\n                });\n            });\n        }\n    }, [products, data, loading, dispatch, id]);\n\n\n    return (\n        <>\n            {currentProduct ? (\n                <div className=\"container\">\n                    <Link to=\"/\">\n                        ← Back to Products\n                </Link>\n                    <div className=\"detail-row\">\n                        <h1>{currentProduct.name}</h1>\n                    </div>\n                    <div className=\"detail-row\">\n                        <p>{currentProduct.description}</p>\n                    </div>\n                    <div className=\"detail-row\">\n\n                        <img src={`/images/${currentProduct.image}`} alt={currentProduct.name} />\n                        </div>\n                        \n                        <div className=\"detail-row\"><strong>Price: ${currentProduct.price}{\" \"}</strong>\n                        <button onClick={addToCart}>\n                            Add To Cart\n                        </button>\n                        </div>\n                    </div>\n               \n            ) : null}\n            {\n                loading ? <img src={spinner} alt=\"loading spinner\" /> : null\n            }\n            <Cart />\n        </>\n    );\n};\nexport default Detail;","D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\pages\\Signup.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\pages\\OrderHistory.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\pages\\Success.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\utils\\GlobalState.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\components\\Footer\\index.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\components\\Nav\\index.js",["110"],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\utils\\mutations.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\utils\\auth.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\utils\\helpers.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\utils\\actions.js",[],"D:\\School\\Class\\Class project 3\\Class Store\\webdev-gifts\\client\\src\\utils\\queries.js",[],"D:\\School\\Class\\Class project 